            "enabled": true,
            "loss_scale": 0,
            "loss_scale_window": 1000,
            "initial_scale_power": 12,
            "hysteresis": 2,
            "min_loss_scale": 1
        },
        "train_iters": 150000,
        "lr_decay_iters": 150000,
        "distributed_backend": "nccl",
        "lr_decay_style": "cosine",
        "warmup": 0.01,
        "checkpoint_factor": 500,
        "eval_interval": 1000,
        "eval_iters": 10,
        "log_interval": 2,
        "steps_per_print": 2,
